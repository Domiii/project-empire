{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],

  "plugins": ["transform-decorators-legacy"],

  "ignore": [
    "*.es5.*"
  ],

  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  }
}