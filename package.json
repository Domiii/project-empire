  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "deploy": "run-s build && firebase deploy",
    "clean": "del-cli public",
    "lint": "eslint -c .eslintrc.yml *.js src server",
    "prebuild": "run-s clean",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:dev",
    "build-test": "cross-env NODE_ENV=test webpack --display-chunks --progress",
    "test": "jest",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test2": "npm run build-test && jest",
    "test:watch": "npm run test -- --inspect-brk --runInBand"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "<rootDir>"
    ],
    "verbosea": true
  },
