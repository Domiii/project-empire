{
  "rules": {
    "dbState": {
      "current": {
        ".read": "auth !== null",
        ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
      }
    },
    "places": {
      ".read": "auth !== null",
      ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
    },
    "missions": {
      ".read": "auth !== null",
      "$missionId": {
        ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
      }
    },
    "projects": {
      ".read": "auth !== null",
      "$projectId": {
        ".write": "auth !== null"
      }
    },
    "meetings": {
      ".read": "auth !== null",
      ".indexOn": [
        "projectId"
      ],
      "$meetingId": {
        ".write": "auth !== null",
        "preparations": {
          "$uid": {}
        }
      }
    },
    "learnerSchedules": {
      ".read": "auth !== null",
      ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
    },
    "learnerEntries": {
      ".read": "auth !== null",
      ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
    },
    "learnerKb": {
      ".read": "auth !== null",
      ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
    },
    "groups": {
      ".read": "auth !== null",
      "$groupId": {
        ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
      }
    },
    "notifications": {
      ".read": "auth !== null",
      "public": {
        ".indexOn": [
          "updatedAt",
          "type"
        ],
        "$notificationId": {
          ".write": "auth !== null && !data.exists() && newData.exists() && newData.child('uid').val() === auth.uid"
        }
      },
      "other": {
        ".indexOn": [
          "updatedAt",
          "type"
        ],
        "$notificationId": {
          ".write": "auth !== null && !data.exists() && newData.exists() && newData.child('uid').val() === auth.uid"
        }
      }
    },
    "users": {
      "public": {
        ".read": "true",
        "$uid": {
          "displayName": {
            ".write": "auth !== null && (($uid === auth.uid) || ((root.child('users').child('public').child(auth.uid).child('role').val() >= 99)))"
          },
          "photoURL": {
            ".write": "auth !== null && (($uid === auth.uid) || ((root.child('users').child('public').child(auth.uid).child('role').val() >= 99)))"
          },
          "role": {
            ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
          },
          "displayRole": {
            ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
          },
          "locale": {
            ".write": "auth !== null && (($uid === auth.uid) || ((root.child('users').child('public').child(auth.uid).child('role').val() >= 99)))"
          },
          "updatedAt": {
            ".write": "auth !== null && (($uid === auth.uid) || ((root.child('users').child('public').child(auth.uid).child('role').val() >= 99)))"
          }
        }
      },
      "private": {
        "$uid": {
          ".read": "auth !== null && (($uid === auth.uid) || ((root.child('users').child('public').child(auth.uid).child('role').val() >= 99)))",
          "updatedAt": {
            ".write": "auth !== null && (($uid === auth.uid) || ((root.child('users').child('public').child(auth.uid).child('role').val() >= 99)))"
          },
          "data": {
            ".write": "auth !== null && (($uid === auth.uid) || ((root.child('users').child('public').child(auth.uid).child('role').val() >= 99)))"
          }
        }
      },
      "prefs": {
        "$uid": {
          ".read": "auth !== null && (($uid === auth.uid) || ((root.child('users').child('public').child(auth.uid).child('role').val() >= 99)))",
          ".write": "auth !== null && (($uid === auth.uid) || ((root.child('users').child('public').child(auth.uid).child('role').val() >= 99)))"
        }
      }
    },
    "_index": {
      "userGroups": {
        "user": {
          ".read": "auth !== null",
          ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
        },
        "group": {
          ".read": "auth !== null",
          ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 99)"
        }
      },
      "projectUsers": {
        "user": {
          ".read": "auth !== null",
          ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 5)"
        },
        "project": {
          ".read": "auth !== null",
          ".write": "auth !== null && (root.child('users').child('public').child(auth.uid).child('role').val() >= 5)"
        }
      }
    },
    "test": {
      ".read": "auth !== null",
      ".write": "auth !== null"
    }
  }
}